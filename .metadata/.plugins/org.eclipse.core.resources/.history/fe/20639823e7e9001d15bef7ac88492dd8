package introtojavabasics;

/**
 * @author homeoffice
 *
 */
public class RelationalDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 3,b = 4;
		boolean r;
		r = (a==b);
		System.out.println(r);
		r = (a==b);
		System.out.println(r);
		r = (a==b);
		System.out.println(r);
		r = (a==b);
		System.out.println(r);
		r = (a==b);
		System.out.println(r);
		r = (a==b);
		System.out.println(r);
	}

}
