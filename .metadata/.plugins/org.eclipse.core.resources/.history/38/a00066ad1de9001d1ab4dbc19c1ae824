package introtojavabasics;

/**
 * @author homeoffice This program prints "Welcome to java"
 */
public class Welcome {

	/**
	 * @param args
	 */
	// Main method id the entry point for the application to run
	// JVM looks for the main method to run an application
	public static void main(String[] args) {
		System.out.println("Welcome to java !!!!");
	}

}
