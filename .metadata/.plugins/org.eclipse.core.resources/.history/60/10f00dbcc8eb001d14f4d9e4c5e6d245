package perscholas.shameena.readingfromconsole;

/**
 * @author homeoffice
 *
 */
public class OperatorsAndNumbersDemo4 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int x,y;
		x=5;y=8;
		int sum = ++x+y;
		System.out.println(sum);
		
		x=5;y=8;
		int sum = x++ + y;
		System.out.println(sum);
		

	}

}
