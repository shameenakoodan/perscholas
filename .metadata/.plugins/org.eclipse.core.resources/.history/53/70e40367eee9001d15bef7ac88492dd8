 */
package introtojavabasics;

/**
 * @author homeoffice
 *
 */
public class AssignmentOperators {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int x=100;
		System.out.println(x);
		x+=5;
		System.out.println("x+=5"+x);
		x+=5;
		System.out.println("x+="+x);
		x+=5;
		System.out.println("x+="+x);
		x+=5;
		System.out.println("x+="+x);

	}

}
