package introtojavabasics;

import java.util.Scanner;

/**
 * @author homeoffice
 *
 */
public class ChineseZodiac {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("Enter the year");
		int year = scanner.nextInt();
		int yearMod12 = year%12;
		String animalString = ""; 
		switch(yearMod12) {
		case 0:
			animalString = "Monkey";
			break;
		case 1:
			animalString = "Rooster";
			break;
		case 0:
			animalString = "Dog";
			break;
		case 0:
			animalString = "Pig";
			break;
		case 0:
			animalString = "Monkey";
			break;
		case 0:
			animalString = "Monkey";
			break;
		case 0:
			animalString = "Monkey";
			break;
		case 0:
			animalString = "Monkey";
			break;
		case 0:
			animalString = "Monkey";
			break;
		case 0:
			animalString = "Monkey";
			break;
		case 0:
			animalString = "Monkey";
			break;
		case 0:
			animalString = "Monkey";
			break;
		}
	}

}
