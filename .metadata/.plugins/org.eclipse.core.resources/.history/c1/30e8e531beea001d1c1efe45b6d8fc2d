 * 
 */
package introtojavabasics;

/**
 * @author homeoffice
 *
 */
public class ArithmeticOperatorsDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int x, y = 10, z = 5;
        x = y + z;
        System.out.println("+ operator resulted in " + x);
        x = y - z;
        System.out.println("- operator resulted in " + x);
        x = y * z;
        System.out.println("* operator resulted in " + x);
        x = y / z;


	}

}
