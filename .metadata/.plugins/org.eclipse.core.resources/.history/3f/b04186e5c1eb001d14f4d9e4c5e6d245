package perscholas.shameena.readingfromconsole;

/**
 * @author homeoffice
 *
 */
public class OperatorsAndNumbersDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int x = 2;
		String binaryString = Integer.toBinaryString(x);
		System.out.println(binaryString);
		x = x<<1;

	}

}
