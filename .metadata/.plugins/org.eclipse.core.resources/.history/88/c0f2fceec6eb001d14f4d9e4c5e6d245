package perscholas.shameena.readingfromconsole;

/**
 * @author homeoffice
 *
 */
public class OperatorsAndNumbersDemo3 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int x=7,y=17,z;
		/*
		 * x = 00111
		 * y = 10001
		 * x | y = 00001 = 1
		 */
		int z = Integer.parseInt((Integer.toBinaryString(x) & Integer.toBinaryString(y)),2);
		System.out.println(z);
	}

}
